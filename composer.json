{
    "name": "zenstruck/backup",
    "description": "Library to create and archive backups",
    "homepage": "http://zenstruck.com/projects/php-backup",
    "keywords": ["backup"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "psr/log": "~1.0",
        "symfony/filesystem": "~2.3|~3.0"
    },
    "require-dev": {
        "symfony/process": "~2.3|~3.0",
        "symfony/console": "~2.5|~3.0",
        "symfony/finder": "~2.5|~3.0",
        "gabrielelana/byte-units": "^0.3.0"
    },
    "autoload": {
        "psr-4": { "Zenstruck\\Backup\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Zenstruck\\Backup\\Tests\\": "tests/" }
    },
    "suggest": {
        "gabrielelana/byte-units": "For pretty filesize display"
    }
}
